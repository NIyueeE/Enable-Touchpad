{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:default",
    "core:app:default",
    "core:event:default"
  ],
  "commands": {
    "allow": [
      "get_settings",
      "save_settings",
      "check_permissions",
      "request_permissions"
    ]
  }
}
